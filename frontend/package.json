{
  "name": "frontend",
  "version": "0.4.0-beta",
  "license": "MIT",
  "author": "Enzo Brancati <enzo.brancati04@gmail.com>",
  "contributors": [
    "Daniele Filareti <daniele.filareti@icloud.com>",
    "Domenico Farano <dodo.farano@gmail.com>",
    "AnielloPio Pentangelo <aniellopiopentangelo2@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ebrancati/OnlineCheckers.org.git"
  },
  "homepage": "https://onlinecheckers.org",
  "description": "Play Italian Checkers online with friends or against AI",
  "keywords": [
    "online",
    "checkers"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:en": "ng serve --configuration development --proxy-config proxy.conf.json",
    "start:it": "ng serve --configuration development-it --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:en": "ng build --configuration production-en",
    "build:it": "ng build --configuration production-it",
    "build:all": "npm run build:en && npm run build:it",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "convert-translations": "node convert-json-to-xlf.js",
    "i18n:extract-and-convert": "npm run extract-i18n && npm run convert-translations",
    "serve:ssr:frontend": "node dist/frontend/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@angular/ssr": "^20.1.3",
    "bootstrap": "^5.3.7",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@angular/localize": "^20.1.3",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "typescript": "^5.8.3"
  }
}